{
  "baseUrl": "https://github.com/luismayta/resume",
  "ignorePatterns": [
    {
      "pattern": "^https://hadenlabs.com"
    },
    {
      "pattern": "^https://github.com/luismayta/resume/compare"
    },
    {
      "pattern": "^mailto:.*@hadenlabs\\.com"
    },
    {
      "pattern": "^http://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs",
      "replacement": "{{BASEURL}}/docs"
    },
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "timeout": "20s",
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 202]
}
